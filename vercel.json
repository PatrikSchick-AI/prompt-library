{
  "buildCommand": "cd prompt-library-app && npm run build && cd .. && cp -r prompt-library-app/api api",
  "outputDirectory": "prompt-library-app/dist",
  "installCommand": "cd prompt-library-app && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, X-Admin-Key"
        }
      ]
    }
  ]
}
